  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "babel-loader": "8.0.6",
    "browser-sync": "2.26.7",
    "webpack": "4.35.0",
    "webpack-cli": "3.3.5",
    "webpack-glsl-loader": "1.0.1"
  },
  "dependencies": {
    "three": "0.104.0"
  }
}
