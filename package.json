{
  "name": "three-webpack-final",
  "version": "1.0.0",
  "description": "threejsによるwebgl開発テンプレート",
  "scripts": {
    "start": "npm run serve & npm run watch",
    "serve": "browser-sync start -c ./config/bs-config.js",
